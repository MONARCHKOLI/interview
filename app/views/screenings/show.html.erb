<%= form_for(:screening, url:  check_in_screenings_path, method: :post) do |f| %>
  <% Screening.questions.each_with_index do |question, index|%>
    <br>
    <br>
    <%= f.label question %>
    <br>
    <br>
    <%= f.select(question, Screening::SCORE.keys, id: "response-#{index}") %>
  <% end %>
  <br>
  <br>
  <br>
  <br>
  <br>
  <%= f.submit "Complete Check" %>
<% end %>